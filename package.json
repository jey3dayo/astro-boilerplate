{
  "type": "module",
  "version": "0.0.1",
  "packageManager": "yarn@3.6.2",
  "engines": {
    "node": ">=16.0.0 <=18.x.x"
  },
  "scripts": {
    "dev": "astro dev",
    "test": "env-cmd -f ./.env.test run-p lint prettier:check depcruise",
    "test:quick": "run-p lint prettier:check depcruise type-check",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --write --cache --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "prettier:check": "prettier --cache --check --ignore-path .prettierignore './**/*.{js,jsx,ts,tsx,json,css}'",
    "type-check": "tsc --noEmit",
    "depcruise": "depcruise -v .dependency-cruiser.js ./{app,components,config,hooks,lib,prisma,styles}",
    "format": "yarn lint:fix && yarn prettier"
  },
  "dependencies": {
    "@astrojs/react": "^2.3.2",
    "@astrojs/tailwind": "^4.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "astro": "^2.10.12",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.268.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "dependency-cruiser": "^13.1.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.47.0",
    "eslint-config-next": "13.4.16",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "prettier-plugin-astro": "^0.11.1",
    "prettier-plugin-tailwindcss": "^0.5.2"
  }
}
